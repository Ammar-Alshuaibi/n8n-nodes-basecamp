  "homepage": "https://github.com/Ammar-Alshuaibi/n8n-nodes-basecamp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ammar-Alshuaibi/n8n-nodes-basecamp.git"
  },
  "bugs": {
    "url": "https://github.com/Ammar-Alshuaibi/n8n-nodes-basecamp/issues"
  },
  "dependencies": {
